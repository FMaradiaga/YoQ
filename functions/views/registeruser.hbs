{{>header}}
<div class="main" id="cuerpo">
    <div class="section">
        <div class="main">
            <div class="row">
                <div class=" col s12 m12">
                    <div class="">
                        <a href="/dash" class="teal-text">Inicio ></a>
                        <a href="/registeruser" class="teal-text"><b>Registro</b></a>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col s12 m12">
                        <div class="title center">
                            <h4>Registro de Establecimiento</h4>
                        </div>
                        <div class="card-content white-text">

                            <div class="row">
                                 <div class="input-field col s4">
                                    <input id="regi_name" type="text" class="validate">
                                    <label for="regi_name">Nombre</label>
                                </div>
                                <div class="input-field col s4">
                                    <input id="regi_email" type="email" class="validate">
                                    <label for="regi_email">email</label>
                                </div>
                               
                                <div class="input-field col s4">
                                    <input id="regi_pass" type="password" class="validate">
                                    <label for="regi_pass">Password</label>
                                </div>
                            </div>

                        </div>
                        <div class="card-action">

                            <button class="waves-effect waves-light btn" onclick="crea_usuario()"><i
                                    class="material-icons left">cloud</i>agregar</button>
                            <a href="/buser" class="waves-effect waves-light btn"><i
                                    class="material-icons left">search</i></a>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

<script type="text/javascript" src="/assets/observador.js"></script>