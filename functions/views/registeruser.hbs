{{>header}}
<div class="main">
    <div class="section">
        <div class="main">
            <div class="row">
                <div class=" col s12 m12">
                    <div class="">
                        <a href="/dash" class="teal-text">Inicio ></a>
                        <a href="/registeruser" class="teal-text"><b>Registro</b></a>
                    </div>
                </div>
            </div>
            <div class="container">
                <div class="row">
                    <div class="col s12 m12">
                        <div class="title center">
                            <h4>Registro de Establecimiento</h4>
                        </div>
                        <div class="card-content white-text">

                            <div class="row">
                                <div class="input-field col s4">
                                    <input placeholder="0508-1991-00278" id="first_name" type="text" class="validate">
                                    <label for="first_name">Identidad</label>
                                </div>
                                <div class="input-field col s4">
                                    <input id="last_name" type="text" class="validate">
                                    <label for="last_name">Nombre</label>
                                </div>
                                <div class="input-field col s4">
                                    <input id="telefono" type="text" class="validate">
                                    <label for="telefono">Telefono</label>
                                </div>
                                <div class="input-field col s6">
                                    <input id="txtuser" type="text" class="">
                                    <label for="txtuser">Usuario</label>
                                </div>
                                <div class="input-field col s6">
                                    <input id="password" type="password" class="validate">
                                    <label for="password">Password</label>
                                </div>
                            </div>

                        </div>
                        <div class="card-action">

                            <button class="waves-effect waves-light btn"><i
                                    class="material-icons left">cloud</i>agregar</button>
                            <a href="/buser" class="waves-effect waves-light btn"><i
                                    class="material-icons left">search</i></a>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>